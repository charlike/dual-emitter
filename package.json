{
  "name": "dual-emitter",
  "version": "0.4.1",
  "description": ":tropical_drink: EventEmitter done right and no dependencies. For nodejs and the browser (>= IE8). Can emit custom or DOM events.",
  "repository": "tunnckoCore/dual-emitter",
  "author": "Charlike Mike Reagent <@tunnckoCore> (http://www.tunnckocore.tk)",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npm run bundle && npm run minify",
    "bundle": "browserify -s DualEmitter -do dist/dual-emitter.js index.js",
    "minify": "uglifyjs -m -c -o dist/dual-emitter.min.js dist/dual-emitter.js",
    "test": "standard && node test.js"
  },
  "dependencies": {},
  "devDependencies": {
    "assertit": "^0.1.0",
    "browserify": "^11.0.1",
    "uglify-js": "^2.4.24"
  },
  "keywords": [
    "bind",
    "browser",
    "chrome",
    "chromium",
    "cross",
    "cross-browser",
    "custom",
    "dom",
    "dual",
    "el",
    "element",
    "elements",
    "emit",
    "emitter",
    "event",
    "events",
    "explorer",
    "firefox",
    "ie",
    "internet",
    "invoke",
    "micro",
    "microjs",
    "mini",
    "mozilla",
    "node",
    "nodejs",
    "off",
    "on",
    "once",
    "one",
    "onetime",
    "trigger",
    "unbind"
  ],
  "standard": {
    "ignore": [
      "dist/**"
    ]
  }
}
